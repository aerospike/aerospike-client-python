dev tun
persist-tun
persist-key
cipher AES-128-CBC
auth SHA1
tls-client
client
remote vpn.aerospike.com 9224 udp
auth-user-pass
remote-cert-tls server

<ca>
-----BEGIN CERTIFICATE-----
%%CA_CERT%%
-----END CERTIFICATE-----
</ca>
setenv CLIENT_CERT 0
<tls-auth>
#
# 2048 bit OpenVPN static key
#
-----BEGIN OpenVPN Static key V1-----
%%PRIVATE_KEY%%
-----END OpenVPN Static key V1-----
</tls-auth>
key-direction 1

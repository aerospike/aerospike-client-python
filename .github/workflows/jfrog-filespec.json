{
    "files": [
        {
            "aql": {
                "items.find": {
                    "repo": "${repo_name}",
                    "path": "${branch}",
                    "$and": [
                        {
                            "name": {
                                "$nmatch": "*(+|.)[a-z]+*"
                            }
                        },
                        {
                            "name": {
                                "$match": "*${commit_sha}*${python_tag}*manylinux*x86_64*.whl"
                            }
                        }
                    ]
                }
            }
        }
    ]
}
